<?xml version="1.0" encoding="ISO-8859-1" ?>
<!--
  This is used to map the model types to
  hibernate types.
-->
<mappings name="Hibernate">
    <mapping>
        <from>datatype::Blob</from>
        <to>org.andromda.persistence.hibernate.usertypes.HibernateByteBlobType</to>
    </mapping>
    <mapping>
        <from>datatype::Clob</from>
        <to>org.andromda.persistence.hibernate.usertypes.HibernateStringClobType</to>
    </mapping>
    <mapping>
        <from>datatype::Hstore</from>
        <to>org.andromda.persistence.hibernate.usertypes.HibernateHStoreUserType</to>
    </mapping>
    <mapping>
        <from>datatype::String[]</from>
        <to>org.andromda.persistence.hibernate.usertypes.HibernateStringArrayUserType</to>
    </mapping>
    <mapping>
        <from>datatype::Integer[]</from>
        <to>org.andromda.persistence.hibernate.usertypes.HibernateIntegerArrayUserType</to>
    </mapping>
    <mapping>
        <from>datatype::Boolean[]</from>
        <to>org.andromda.persistence.hibernate.usertypes.HibernateBooleanArrayUserType</to>
    </mapping>
</mappings>

